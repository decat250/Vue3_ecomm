AU,aAAO,CAACC,GAAR,CAAYU,GAAG,CAACnB,WAAJ,CAAgBqB,MAA5B;AACA,UAAIzB,IAAG,GAAI,EAAX;;AACA,WAAK,IAAI0B,IAAI,CAAb,EAAgBA,IAAIH,GAAG,CAACnB,WAAJ,CAAgBqB,MAApC,EAA4CC,CAAC,EAA7C,EAAiD;AAC/C1B,YAAI,CAAC2B,IAAL,CAAU,KAAI,CAACvB,WAAL,CAAiBsB,CAAjB,CAAV;AACF;;AACAd,aAAO,CAACC,GAAR,CAAYb,IAAZ;AACA,WAAI,CAACQ,eAAL,GAAuBR,IAAvB,CAXa,CAYb;AACD,KAvBH;AAwBD,GArD4B;AAsD7B4B,SAAO,EAAE;AACPC,YADO,sBACI;AACT,UAAI7B,IAAG,GAAI,EAAX;;AAEA,UAAI,KAAKE,IAAL,IAAa,KAAKD,SAAtB,EAAiC;AAC/B,aAAK,IAAI6B,IAAI,CAAC,KAAK5B,IAAL,GAAY,CAAb,IAAkB,EAA/B,EAAmC4B,IAAI,KAAK1B,WAAL,CAAiBqB,MAAxD,EAAgEK,CAAC,EAAjE,EAAqE;AACnE9B,cAAI,CAAC2B,IAAL,CAAU,KAAKvB,WAAL,CAAiB0B,CAAjB,CAAV;AACF;AACF,OAJA,MAIO,IAAI,KAAK5B,IAAL,IAAa,CAAjB,EAAoB;AACzB,aAAK,IAAIwB,IAAI,CAAb,EAAgBA,IAAI,KAAKxB,IAAL,GAAY,EAAhC,EAAoCwB,CAAC,EAArC,EAAyC;AACvC1B,cAAI,CAAC2B,IAAL,CAAU,KAAKvB,WAAL,CAAiBsB,CAAjB,CAAV;AACF;AACF,OAJO,MAIA;AACL,aAAK,IAAIK,IAAI,CAAC,KAAK7B,IAAL,GAAY,CAAb,IAAkB,EAA/B,EAAmC6B,IAAI,KAAK7B,IAAL,GAAY,EAAnD,EAAuD6B,CAAC,EAAxD,EAA4D;AAC1D/B,cAAI,CAAC2B,IAAL,CAAU,KAAKvB,WAAL,CAAiB2B,CAAjB,CAAV;AACF;AACF;;AAEA,WAAKvB,eAAL,GAAuBR,IAAvB;AACD;AAnBM;AAtDoB,CAAD,CAA9B","names":["h","defineComponent","data","totalpage","page","bannerimg","productlist","menuOptions","label","key","productlistshow","setup","renderMenuLabel","option","console","log","href","target","mounted","fetch","method","headers","Accept","then","json","ret","type","length","j","push","methods","showitem","i","z"],"sourceRoot":"","sources":["/Users/decat/Vue3_ecomm/src/views/Home.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <n-carousel autoplay show-dots=\"false\">\n      <img\n        v-for=\"item in bannerimg\"\n        v-bind:key=\"item.banner_id\"\n        class=\"carousel-img\"\n        v-bind:src=\"item.item\"\n      />\n    </n-carousel>\n    <n-space vertical>\n      <n-layout has-sider>\n        <n-layout-sider\n          bordered\n          collapse-mode=\"width\"\n          :collapsed-width=\"64\"\n          :width=\"240\"\n          :collapsed=\"trigger\"\n          show-trigger\n          @collapse=\"true\"\n          @expand=\"true\"\n        >\n          <n-menu\n            :collapsed=\"collapsed\"\n            :collapsed-width=\"64\"\n            :collapsed-icon-size=\"22\"\n            :options=\"menuOptions\"\n            :render-label=\"renderMenuLabel\"\n            :render-icon=\"renderMenuIcon\"\n            :expand-icon=\"expandIcon\"\n          />\n        </n-layout-sider>\n        <n-layout>\n          <span>\n            <div class=\"row\" style=\"margin-top: 10px\">\n              <router-link\n                :to=\"`/ProductInfo/${item.product_id}`\"\n                v-for=\"item in productlistshow\"\n                v-bind:key=\"item.id\"\n                class=\"col-4\"\n                style=\"text-decoration: none\"\n              >\n                <n-card\n                  title=\"\"\n                  :segmented=\"{\n                    content: true,\n                    footer: 'soft',\n                  }\"\n                >\n                  <template #header-extra> #header-extra </template>\n                  <img class=\"img-fluid\" v-bind:src=\"item.product_img\" />\n\n                  <template #footer> {{ item.product_name }}</template>\n                  <template #action>\n                    <h5 style=\"color: red\">\n                      NT $ {{ item.product_price }}\n                    </h5></template\n                  >\n                </n-card>\n              </router-link>\n\n              <div\n                class=\"col-12\"\n                style=\"margin-top: 30px; margin-left: auto; margin-right: auto\"\n              >\n                <n-pagination\n                  v-model:page=\"page\"\n                  @update:page=\"showitem\"\n                  :page-count=\"totalpage\"\n                  show-quick-jumper\n                />\n              </div></div\n          ></span>\n        </n-layout>\n      </n-layout>\n    </n-space>\n  </div>\n</template>\n<style>\n.carousel-img {\n  width: 100%;\n  height: 300px;\n\n  object-fit: cover;\n}\n</style>\n<script>\nimport { h, defineComponent } from \"vue\";\n\n\nexport default defineComponent({\n  data() {\n    return {\n      totalpage: 0,\n      page: 1,\n      bannerimg: [],\n      productlist: [],\n      menuOptions: [\n        {\n          label: \"Hear the Wind Sing\",\n          key: \"d\",\n        },\n      ],\n      productlistshow: [],\n    };\n  },\n\n  setup() {\n    return {\n      renderMenuLabel(option) {\n        console.log(option);\n        if (\"href\" in option) {\n          return h(\"a\", { href: option.href, target: \"_blank\" }, option.label);\n        }\n        return option.label;\n      },\n    };\n  },\n  mounted() {\n    fetch(\"http://127.0.0.1/api/getproductlist/\", {\n      method: \"get\",\n      headers: {\n        Accept: \"application/json\",\n        \"Content-Type\": \"application/json\",\n      },\n    })\n      .then((data) => {\n        return data.json();\n      })\n      .then((ret) => {\n        this.menuOptions = ret.type\n        this.bannerimg = ret.bannerimg;\n        this.productlist = ret.productlist;\n        this.totalpage = ret.page;\n        console.log(ret.productlist.length);\n        var data = [];\n        for (var j = 0; j < ret.productlist.length; j++) {\n          data.push(this.productlist[j]);\n        }\n        console.log(data);\n        this.productlistshow = data;\n        //this.productlistshow = ret.productlist;\n      });\n  },\n  methods: {\n    showitem() {\n      var data = [];\n\n      if (this.page == this.totalpage) {\n        for (var i = (this.page - 1) * 10; i < this.productlist.length; i++) {\n          data.push(this.productlist[i]);\n        }\n      } else if (this.page == 1) {\n        for (var j = 0; j < this.page * 10; j++) {\n          data.push(this.productlist[j]);\n        }\n      } else {\n        for (var z = (this.page - 1) * 10; z < this.page * 10; z++) {\n          data.push(this.productlist[z]);\n        }\n      }\n\n      this.productlistshow = data;\n    },\n  },\n});\n</script>"]}]}